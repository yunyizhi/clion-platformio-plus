<?xml version="1.0" encoding="UTF-8" ?>
<IniMeta>
    <Section name="env:" desc="[env:*]">
        <!-- Platform options-->
        <Key name="platform">
        </Key>
        <Key name="platform_packages"/>
        <Key name="framework"/>
        <Key name="board"/>
        <Key name="board_build.mcu"/>
        <Key name="board_build.f_cpu"/>
        <Key name="board_build.ldscript"/>
        <!--  Build options-->
        <Key name="build_type">
            <value toolTip="
            Default configuration. A “release” configuration of your firmware/program does not contain symbolic debug information and is optimized for the firmware size or speed (depending on Development Platforms)"
            >release
            </value>
            <value toolTip="A “test” configuration extends a build environment with PIO_UNIT_TESTING macro and with extra flags provided by the Unit Testing framework"
            >test
            </value>
            <value toolTip="A “debug” configuration of your firmware/program is compiled with full symbolic debug information and no optimization. Optimization complicates debugging because the relationship between source code and generated instructions is more complex"
            >debug
            </value>
        </Key>
        <Key name="build_flags">
            <!--arg 标签将处理自动补全的-重复问题 -->
            <arg platform="espressif32" framework="arduino">-D BOARD_HAS_PSRAM</arg>
            <arg platform="espressif32" framework="arduino">-mfix-esp32-psram-cache-issue</arg>
            <arg platform="espressif32" framework="arduino">-mfix-esp32-psram-cache-strategy=memw</arg>
            <arg platform="espressif32" framework="arduino">-D CORE_DEBUG_LEVEL=ARDUHAL_LOG_LEVEL_INFO</arg>
            <arg platform="espressif32" framework="arduino">-D CONFIG_ARDUHAL_LOG_COLORS=1</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_USB_CDC_ON_BOOT=1</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_USB_MSC_ON_BOOT=1</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_LOOP_STACK_SIZE=</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_RUNNING_CORE=</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_EVENT_RUNNING_CORE=</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_SERIAL_EVENT_TASK_STACK_SIZE=</arg>
            <arg platform="espressif32" framework="arduino">-D ARDUINO_SERIAL_EVENT_TASK_PRIORITY=</arg>

        </Key>
        <Key name="build_src_flags"/>
        <Key name="build_unflags"/>
        <Key name="build_src_filter"/>
        <Key name="targets">
            <value toolTip="automatically start pio device monitor after successful build operation. You can configure monitor using Monitor options.">monitor</value>
            <value toolTip="dump current build environment">envdump</value>
        </Key>
        <!--  Library options-->
        <Key name="lib_deps"/>
        <Key name="lib_ignore"/>
        <Key name="lib_extra_dirs"/>
        <Key name="lib_ldf_mode"/>
        <Key name="lib_compat_mode"/>
        <Key name="lib_archive"/>
        <!-- Upload options-->
        <Key name="upload_port"/>
        <Key name="upload_protocol"/>
        <Key name="upload_speed">
            <value>300</value>
            <value>600</value>
            <value>750</value>
            <value>1200</value>
            <value>2400</value>
            <value>4800</value>
            <value>9600</value>
            <value>14400</value>
            <value>19200</value>
            <value>28800</value>
            <value>38400</value>
            <value>57600</value>
            <value>115200</value>
            <value>128000</value>
            <value>230400</value>
            <value>256000</value>
            <value>468000</value>
            <value>512000</value>
            <value>921600</value>
            <value>1000000</value>
            <value>1024000</value>
            <value>2000000</value>
        </Key>
        <Key name="upload_flags"/>
        <Key name="upload_resetmethod"/>
        <Key name="upload_command"/>
        <!-- Monitor options-->
        <Key name="monitor_port"/>
        <Key name="monitor_speed">
            <value>300</value>
            <value>600</value>
            <value>750</value>
            <value>1200</value>
            <value>2400</value>
            <value>4800</value>
            <value>9600</value>
            <value>14400</value>
            <value>19200</value>
            <value>28800</value>
            <value>38400</value>
            <value>57600</value>
            <value>115200</value>
            <value>128000</value>
            <value>230400</value>
            <value>256000</value>
            <value>468000</value>
            <value>512000</value>
            <value>921600</value>
            <value>1000000</value>
            <value>1024000</value>
            <value>2000000</value>
        </Key>
        <Key name="monitor_parity">
            <value>N</value>
            <value>E</value>
            <value>O</value>
            <value>S</value>
            <value>M</value>
        </Key>
        <Key name="monitor_filters">
            <value toolTip="Remove typical terminal control codes from input">default</value>
            <value toolTip="Apply different colors for received and echo">colorize</value>
            <value toolTip="Print what is sent and received">debug</value>
            <value toolTip="Do-nothing: forward all data unchanged">direct</value>
            <value toolTip="Show a hexadecimal representation of the data (code point of each character)">hexlify
            </value>
            <value toolTip="Log data to a file &quot;platformio-device-monitor-%date%.log&quot; located in the current working directory">
                log2file
            </value>
            <value toolTip="Remove all control codes, incl. CR+LF">nocontrol</value>
            <value toolTip="Show decimal code for all non-ASCII characters and replace most control codes">printable
            </value>
            <value toolTip="Add timestamp with milliseconds for each new line">time</value>
            <value toolTip="Send a text to device on ENTER">send_on_enter</value>
            <value platform="espressif32"
                   toolTip="Custom filter for `platform_espressif32` which decodes crash exception">
                esp32_exception_decoder
            </value>
            <value platform="espressif8266"
                   toolTip="Custom filter for `platform_espressif8266` which decodes crash exception">
                esp8266_exception_decoder
            </value>
        </Key>
        <Key name="board_build.f_flash" platform="espressif32" framework="arduino"/>
        <Key name="board_build.arduino.memory_type" platform="espressif32" framework="arduino">
            <value>dio_qspi</value>
            <value>opi_opi</value>
            <value>qio_opi</value>
            <value>dio_opi</value>
            <value>opi_qspi</value>
            <value>qio_qspi</value>
        </Key>
        <Key name="board_build.flash_mode" platform="espressif32" framework="arduino">
            <value>dio</value>
            <value>opi</value>
            <value>qio</value>
        </Key>
        <Key name="board_upload.flash_size" platform="espressif32" framework="arduino">
            <value>4MB</value>
            <value>8MB</value>
            <value>16MB</value>
            <value>32MB</value>
            <value>64MB</value>
            <value>128MB</value>
        </Key>
        <Key name="board_upload.maximum_size" platform="espressif32" framework="arduino"/>
        <Key name="board_build.partitions" platform="espressif32" framework="arduino"/>
        <Key name="board_build.extra_flags" platform="espressif32" framework="arduino">
            <value>-D ARDUINO_USB_MODE=0</value>
            <value>-D ARDUINO_USB_MODE=1</value>
        </Key>
        <Key name="monitor_rts">
            <value>0</value>
            <value>1</value>
        </Key>
        <Key name="monitor_dtr">
            <value>0</value>
            <value>1</value>
        </Key>
        <Key name="monitor_eol">
            <value>CR</value>
            <value>LF</value>
            <value>CRLF</value>
        </Key>
        <Key name="monitor_raw">
            <value>yes</value>
            <value>no</value>
        </Key>
        <Key name="monitor_echo"/>
        <Key name="monitor_encoding">
            <value>hexlify</value>
            <value>UTF-8</value>
        </Key>
        <!-- Check options-->
        <Key name="check_tool"/>
        <Key name="check_patterns"/>
        <Key name="check_flags"/>
        <Key name="check_severity"/>
        <Key name="check_skip_packages"/>
        <!--Test options -->
        <Key name="test_framework"/>
        <Key name="test_filter"/>
        <Key name="test_ignore"/>
        <Key name="test_port"/>
        <Key name="test_speed"/>
        <Key name="test_build_src"/>
        <Key name="test_testing_command"/>
        <!-- Debugging options-->
        <Key name="debug_tool"/>
        <Key name="debug_build_flags"/>
        <Key name="debug_init_break"/>
        <Key name="debug_init_cmds"/>
        <Key name="debug_extra_cmds"/>
        <Key name="debug_load_cmds"/>
        <Key name="debug_load_mode">
            <value toolTip="load for the each debugging session, default">always</value>
            <value toolTip="load only when firmware was modified">modified</value>
            <value toolTip="do not load firmware automatically. You are responsible to pre-flash target with debugging firmware in this case.">
                manual
            </value>
        </Key>
        <Key name="debug_server"/>
        <Key name="debug_port"/>
        <Key name="debug_speed"/>
        <Key name="debug_svd_path"/>
        <Key name="debug_server_ready_pattern"/>
        <Key name="debug_test"/>
        <!-- Advanced options-->
        <Key name="extends"/>
        <Key name="extra_scripts"/>
    </Section>
    <Section name="platformio" desc="[platformio]">
        <!-- Generic options-->
        <Key name="description"/>
        <Key name="default_envs"/>
        <Key name="extra_configs"/>
        <!-- Directory options-->
        <Key name="core_dir"/>
        <Key name="globallib_dir"/>
        <Key name="platforms_dir"/>
        <Key name="packages_dir"/>
        <Key name="cache_dir"/>
        <Key name="build_cache_dir"/>
        <Key name="workspace_dir"/>
        <Key name="build_dir"/>
        <Key name="libdeps_dir"/>
        <Key name="include_dir"/>
        <Key name="src_dir"/>
        <Key name="lib_dir"/>
        <Key name="data_dir"/>
        <Key name="test_dir"/>
        <Key name="boards_dir"/>
        <Key name="monitor_dir"/>
        <Key name="shared_dir"/>
    </Section>
    <Section name="env" desc="[env]"/>
</IniMeta>